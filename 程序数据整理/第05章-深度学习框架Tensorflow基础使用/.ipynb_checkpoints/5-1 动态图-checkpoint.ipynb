    "import tensorflow as tf\n",
    "# 创建一个常量\n",
    "m1 = tf.constant([[4,4]])\n",
    "# 创建一个常量\n",
    "m2 = tf.constant([[2],[3]])\n",
    "# 创建一个矩阵乘法，把m1和m2传入\n",
    "product = tf.matmul(m1,m2)\n",
    "# 打印结果\n",
    "print(product)"
