    "import tensorflow as tf\n",
    "import json\n",
    " \n",
    "# 读入json文件\n",
    "with open('model.json') as m:\n",
    "    json_config = json.load(m)\n",
    "    \n",
    "# 载入json模型结构得到模型model\n",
    "model = tf.keras.models.model_from_json(json_config)\n",
    "\n",
    "# summary用于查看模型结构\n",
    "model.summary()"
   ]
